{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "classic",
		"noImplicitAny": false,
		"removeComments": false,
		"preserveConstEnums": true,
		"target": "es5",
		"sourceMap": false,
		"experimentalDecorators": true,
		"declaration": true
	},
	"exclude": [
		"node_modules",
		"typings"
	]
}